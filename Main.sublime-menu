[
    {
        "mnemonic": "n",
        "caption": "Preferences",
        "id": "preferences",
        "children": [
            {
                "mnemonic": "P",
                "caption": "Package Settings",
                "id": "package-settings",
                "children": [
                    {
                        "caption": "myChatGPT",
                        "children": [
                            {
                                "caption": "Open Command Palette",
                                "command": "chat_gpt"
                            },
                            {
                                "caption": "Settings",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/ChatGPT/ChatGPT.sublime-settings",
                                    "default": "// Settings in here override those in \"ChatGPT/ChatGPT.sublime-settings\"\n{\n\t$0\n}\n"
                                }
                            },
                            {
                                "caption": "Key Bindings",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/ChatGPT/ChatGPT.sublime-keymap",
                                    "user_file": "${packages}/User/Default ($platform).sublime-keymap",
                                    "default": "[\n\t{\n\t\t\"keys\": [\"alt+shift+c\"],\n\t\t\"command\": \"chat_gpt\"\n\t}\n]"
                                }
                            },
                            {
                                "caption": "README",
                                "command": "open_file",
                                "args": {
                                    "file": "${packages}/ChatGPT/README.md",
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
]
